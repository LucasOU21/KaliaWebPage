{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/send-calculator-email.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "EMAIL_USER": "$EMAIL_USER",
    "EMAIL_PASS": "$EMAIL_PASS",
    "EMAIL_HOST": "$EMAIL_HOST",
    "EMAIL_PORT": "$EMAIL_PORT"
  }
}